{
  "expo": {
    "name": "Eco Quiz",
    "slug": "Eco Quiz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/idea.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/idea.png",
        "backgroundColor": "#FFF1E4"
      }
    },
    "web": {
      "favicon": "./src/assets/idea.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
